#include <stdio.h>
#define _CRT_SECURE_NO_WARNINGS

int main() {

    // 변수 선언 및 초기화
    int a = 100;
    int b = 200;
    int tmp;

    // 현재 값 출력
    printf("a값 입력 : 100\n");

    printf("b값 입력 : 200\n");

    // 두 변수의 값 바꾸기
    tmp = a;
    a = b;
    b = tmp;

    // 바뀐 값 출력
    printf("바뀐 값 a는 %d, b는 %d\n", a, b);

    return 0;

}
